<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
	<comment>
		Preset for A4: 32 character wide (29 rows)
	</comment>
	
	<!-- General appearance
	<entry key="template">book</entry> -->

	<!-- Layout performer -->
	<entry key="page-height">29</entry>
	<entry key="page-width">32</entry>
	<entry key="inner-margin">2</entry>
	<entry key="outer-margin">2</entry>
	<entry key="row-spacing">2</entry>

	<!-- Splitter -->
	<entry key="splitterMax">50</entry>

</properties>
