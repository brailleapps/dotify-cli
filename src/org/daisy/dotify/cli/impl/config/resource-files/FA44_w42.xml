<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
	<comment>
		Preset for FA44: 42 character wide (29 rows)
	</comment>

	<!-- Layout performer -->
	<entry key="page-height">29</entry>
	<entry key="page-width">42</entry>
	<entry key="inner-margin">3</entry>
	<entry key="outer-margin">3</entry>
	<entry key="row-spacing">1</entry>

	<!-- Splitter -->
	<entry key="splitterMax">50</entry>

</properties>
